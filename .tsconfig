{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "es5",
    "downlevelIteration": true,
    "importHelpers": true,
    "noLib": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "experimentalDecorators": true
  }
}
