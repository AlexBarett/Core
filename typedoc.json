{
  "name": "V4Fire",
  "tsconfig": "./tsconfig.json",
  "mode": "modules",
  "src": ["./src", "./index.d.ts"],
  "out": "./doc",
  "readme": "./README.md",
  "hideGenerator": true,
  "ignoreCompilerErrors": false,
  "includes": "./src",
  "includeDeclarations": true,
  "excludePrivate": true,
  "exclude": [
    "**/engines/**",
    "**/node_modules/!(eventemitter2)/**",
    "src/core/prelude/*.ts",
    "src/core/prelude/i18n/!(index).ts",
    "src/core/prelude/!(extend|env|i18n)/**"
  ]
}
